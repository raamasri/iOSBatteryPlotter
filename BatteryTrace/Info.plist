<plist version="1.0">
<dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.batterytrace.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>batterytrace</string>
			</array>
		</dict>
